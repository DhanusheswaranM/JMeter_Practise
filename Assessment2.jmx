            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Extracting amount">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.regex.*;

String response = prev.getResponseDataAsString();

Pattern patternPrice = Pattern.compile(&quot;&lt;p&gt;Price: (.+?)&lt;/p&gt;&quot;);
Matcher matcherPrice = patternPrice.matcher(response);

Double price = 0.0;
if (matcherPrice.find()) {
    price = Double.parseDouble(matcherPrice.group(1));
    vars.put(&quot;price&quot;, String.valueOf(price));
}

Pattern patternAttributeFee = Pattern.compile(&quot;&lt;p&gt;Arbitrary Fees and Taxes: (.+?)&lt;/p&gt;&quot;);
Matcher matcherAttributeFee = patternAttributeFee.matcher(response);

Double attributeFee = 0.0;
if (matcherAttributeFee.find()) {
    attributeFee = Double.parseDouble(matcherAttributeFee.group(1));
    vars.put(&quot;AttributeFee&quot;, String.valueOf(attributeFee));
}

Double total = price + attributeFee;
vars.put(&quot;TotalCost&quot;, String.valueOf(total));
</stringProp>
            </BeanShellPostProcessor>
