            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Flight ID">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.regex.*;

String response = prev.getResponseDataAsString();

Pattern patternID = Pattern.compile(&quot;value=\&quot;(.+?)\&quot; name=\&quot;flight\&quot;&quot;);

Matcher matcherID = patternID.matcher(response);

    
if (matcherID.find()) {
    String flightID = matcherID.group(1);
    
    vars.put(&quot;FlightID&quot;, flightID);
    
    log.info(&quot;Extracted flight ID : &quot; + flightID);
}

Pattern patternprice = Pattern.compile(&quot;value=\&quot;(.+?)\&quot; name=\&quot;price\&quot;&quot;);

Matcher matcherprice = patternprice.matcher(response);

    
if (matcherprice.find()) {
    String flightprice = matcherprice.group(1);
    
    vars.put(&quot;FlightPrice&quot;, flightprice);
    
    log.info(&quot;Extracted flight Price : &quot; + flightprice);
}

Pattern patternAirline = Pattern.compile(&quot;value=\&quot;(.+?)\&quot; name=\&quot;airline\&quot;&quot;);

Matcher matcherAirline = patternAirline.matcher(response);

    
if (matcherAirline.find()) {
    String flightAirline = matcherAirline.group(1);
    
    vars.put(&quot;FlightAirline&quot;, flightAirline);
    
    log.info(&quot;Extracted flight Airline : &quot; + flightAirline);
}
</stringProp>
