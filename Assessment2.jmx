            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Route Extracting">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">String response = prev.getResponseDataAsString();

int begin = response.indexOf(&quot;Flights from &quot;) + (&quot;Flights from &quot;).length();
int end = response.indexOf(&quot;:&quot;,begin);

String TravelRoute = response.substring(begin,end);

